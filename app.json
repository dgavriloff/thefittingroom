{
  "expo": {
    "name": "the fitting room",
    "slug": "fitting-room",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "thefittingroom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dgavriloff.thefittingroom",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "the fitting room needs access to your camera so you can capture photos of your clothes and yourself for virtual try-on sessions.",
        "NSPhotoLibraryUsageDescription": "the fitting room needs access to your photo library to let you select existing photos of clothes and models for virtual try-on.",
        "NSPhotoLibraryAddUsageDescription": "the fitting room needs access to your photo library to save your generated virtual try-on images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "46830fd1-7ac6-4537-a9fd-2d45512e8fe2"
      }
    }
  }
}